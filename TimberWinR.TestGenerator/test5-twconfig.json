{
    "TimberWinR": {
        "Inputs": {
            "Udp": [
                {
                    "_comment": "Output from NLog",
                    "port": 5140,
                    "add_field": [
                        "Environment",
                        "PLANT_TST_TIMBERWINR"
                    ],
                     "rename": [
                        "Type",
                        "type"
                    ]
                }
            ],
            "TailFiles": [
                {
                    "interval": 5,
                    "logSource": "log files",
                    "location": "*.jlog",
                    "recurse": -1
                }
            ]
        },
        "Outputs": {
            "Redis": [
                {
                    "_comment": "Change the host to your Redis instance",
                    "port": 6379,
                    "batch_count": 500,
                    "interval":  1000,
                    "threads": 4,
                    "host": [
                        "tstlexiceapp006.mycompany.svc"
                    ]
                }
            ]
        }
    }
}
