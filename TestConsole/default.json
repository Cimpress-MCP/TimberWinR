{
    "TimberWinR": {
        "Inputs": {
            "WindowsEvents": [
                {
                    "source": "System",
                    "binaryFormat": "PRINT",
                    "resolveSIDS": true
                }
            ]
        },
        "Filters": [
            {
                "grok": {
                    "condition": "\"[type]\" == \"Win32-Eventlog\"",
                    "match": [
                        "Message",
                        ""
                    ]
                }
            }
        ],
        "Outputs": {
            "Elasticsearch": [
                {
                    "threads": 1,
                    "interval": 5000,
                    "host": [
                        "11.41.25.64"
                    ],
                    "index": "logging-%{yyyy.MM.dd}"
                }
            ]
        }
    }
}